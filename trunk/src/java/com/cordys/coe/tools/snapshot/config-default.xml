<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Config xmlns="http://schemas.cordys.csc.com/snapshotgrabber">
	<Username>admin</Username>
	<Password>admin</Password>
	<ServerList>
		<Server>
			<Name>localhost</Name>
			<Port>1099</Port>
		</Server>
	</ServerList>
	<ServiceGroupList>
		<ServiceGroup>
			<Name>.+</Name>
			<ServiceContainerList>
				<ServiceContainer>
					<Name>.+</Name>
					<JMXCounterList>
						<JMXCounter>
							<Domain>java.lang</Domain>
							<NameProperties>
								<NameProperty>
									<Key>type</Key>
									<Value>Runtime</Value>
								</NameProperty>
							</NameProperties>
							<Property>Name</Property>
							<CounterType>ATTRIBUTE</CounterType>
							<DataHandler>com.cordys.coe.tools.snapshot.data.handler.HostPIDInfo</DataHandler>
						</JMXCounter>
						<JMXCounter>
							<Domain>com.cordys</Domain>
							<DataCollector>com.cordys.coe.tools.snapshot.data.collector.WorkerThreadCollector</DataCollector>
						</JMXCounter>
						<JMXCounter>
							<Domain>com.cordys</Domain>
							<DataCollector>com.cordys.coe.tools.snapshot.data.collector.MemoryCollector</DataCollector>
						</JMXCounter>
						<JMXCounter>
							<Domain>com.cordys</Domain>
							<DataCollector>com.cordys.coe.tools.snapshot.data.collector.DBPoolCollector</DataCollector>
						</JMXCounter>
                        <JMXCounter>
                            <Domain>com.cordys</Domain>
                            <DataCollector>com.cordys.coe.tools.snapshot.data.collector.JMXWSICollector</DataCollector>
                        </JMXCounter>
						<JMXCounter>
							<Domain>java.lang</Domain>
							<NameProperties>
								<NameProperty>
									<Key>type</Key>
									<Value>Threading</Value>
								</NameProperty>
							</NameProperties>
							<Property>dumpAllThreads</Property>
							<CounterType>OPERATION</CounterType>
							<ParameterList>
								<Parameter>
									<Value>true</Value>
								</Parameter>
								<Parameter>
									<Value>true</Value>
								</Parameter>
							</ParameterList>
							<DataHandler>com.cordys.coe.tools.snapshot.data.handler.DumpAllThreads</DataHandler>
						</JMXCounter>
					</JMXCounterList>
				</ServiceContainer>
			</ServiceContainerList>
		</ServiceGroup>
	</ServiceGroupList>
</Config>
